msgid "It's time!"
msgstr "Het is tijd!"

#: translations/strings.xml:1692(item)
msgid "Excuse me! Time for"
msgstr "Sorry hoor! Nu moet"

#: translations/strings.xml:1693(item)
msgid "You free? Time to"
